<?xml version="1.0" encoding="utf-8"?>

<Robot name="Hekateros">

  <KinBody name="Hekateros">

    <!--arm-->
    <KinBody file="./hekateros.arm.kinbody.xml"></KinBody>

    <!--end effector-->
    <KinBody file="./hekateros.manip.kinbody.xml">
      <Translation>0.0 .02 0.69</Translation>
      <quat>0 1 0 0 </quat>
      <quat>0 0 0 1 </quat>
    </KinBody>

    <joint type="hinge" name="wristRotate" enabled="true" circular="true">
      <Body>endEffInterface</Body>
      <Body>endEffector</Body>
      <offsetFrom>endEffector</offsetFrom>
    </joint>

  </KinBody>

  <Manipulator name="arm">
    <base> Base1 </base>
    <effector> endEffector </effector>
    <joints> baseRotate shoulder elbow wristTilt wristRotate </joints>
    <closingdirection>1 1 1 0 1</closingdirection>
    <direction>0 0 1</direction>
  </Manipulator>

</Robot>
